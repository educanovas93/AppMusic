

.combo-box .combo-box-popup .list-view {
    -fx-background-color: #282828;
    -fx-text-fill: #a4a4a4;
    -fx-font-family: "Calibri" ;
  	-fx-background-radius: 5 5 5 5;
  	-fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 20, 0.0 , 0 , 0 );

}


.combo-box .combo-box-popup .list-cell {
    -fx-background-color: #282828;
    -fx-text-fill: #a4a4a4;
    -fx-font-family: "Calibri" ;
}
.combo-box .combo-box-popup .list-cell:hover {
    -fx-text-fill: white ;
    -fx-background-color: #404040 ;
}
.jfx-combo-box .button {
	-fx-prompt-text-fill: white;
    -fx-text-fill: white;
	
}


.jfx-combo-box{
    -jfx-focus-color: #49bc64;
    -jfx-unfocus-color: white;
    -fx-prompt-text-fill: white;
    -fx-text-fill: white;
}

.root .jfx-combo-box .list-view .text-field{
    -fx-prompt-text-fill: black;
    -fx-text-fill: black;
}

.jfx-combo-box,
.jfx-combo-box:focused,
.jfx-combo-box:editable,
.jfx-combo-box:editable:focused{
    -fx-background-color: TRANSPARENT, TRANSPARENT, TRANSPARENT, TRANSPARENT;
    -fx-background-radius: 3px;
    -fx-background-insets: 0px;
}

.jfx-combo-box .combo-box-button-container{
    -fx-background-color:TRANSPARENT;
}

.jfx-combo-box:error {
    -jfx-focus-color: #D34336;
    -jfx-unfocus-color: #D34336;
}

.jfx-combo-box > .arrow-button,
.jfx-combo-box:editable > .arrow-button,
.jfx-combo-box:editable:focused > .arrow-button{
    -fx-background-color: TRANSPARENT;
}

.combo-box:contains-focus {
    -fx-background-color: -fx-focus-color;
    -fx-background-insets: -0.2;
    -fx-background-radius: 3;
    
}